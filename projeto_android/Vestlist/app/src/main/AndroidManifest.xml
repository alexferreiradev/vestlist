<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alex.vestlist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.view.TeacherActivity"
            android:parentActivityName=".ui.view.MainActivity"/>
        <activity android:name=".ui.view.ListActivity"
            android:parentActivityName=".ui.view.TeacherActivity"/>
        <activity android:name=".ui.view.DoubtActivity"
            android:parentActivityName=".ui.view.TeacherActivity"/>
        <activity android:name=".ui.view.AddOrEditDoubtActivity"
            android:parentActivityName=".ui.view.DoubtActivity"></activity>
    </application>

</manifest>